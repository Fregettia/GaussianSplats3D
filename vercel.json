{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/demo",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "/lib/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/lib/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 